<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#212D47"
    tools:context=".Mokhaberat2_Activity" >

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="63dp"
        android:layout_height="61dp"

        app:srcCompat="@drawable/button_ovil"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="95dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="45dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/barg"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="102dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="4dp"
        android:background="#DA0E172C"
        android:padding="10dp"
        android:text="قبض تلفن ثابت"
        android:textAlignment="center"
        android:textColor="#9B9D9F"
        android:textSize="28dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/hidden_button_back"
        android:layout_width="93dp"
        android:layout_height="62dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:importantForAccessibility="no"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView17"
        app:layout_constraintEnd_toEndOf="@+id/imageView17"
        app:layout_constraintStart_toStartOf="@+id/imageView17"
        app:layout_constraintTop_toTopOf="@+id/imageView17" />
    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="37dp"
        android:layout_height="41dp"
        app:srcCompat="@drawable/fii"
        tools:layout_editor_absoluteX="344dp"
        tools:layout_editor_absoluteY="12dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/row"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="215dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/button_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_margin="32dp"
            android:background="@drawable/button_background_m"

            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_editor_absoluteX="32dp">

            <Button
                android:id="@+id/hidden_button"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:importantForAccessibility="no"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ثبت"
                android:textColor="#ffffff"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="403dp"
            android:layout_height="50dp"
            android:layout_margin="8dp"
            android:padding="10dp"
            android:text="قبض میان دوره و پایان دوره شما به شرح زیر است :"
            android:textColor="#9B9D9F"
            android:textSize="18dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/TextView_m"
            android:layout_width="359dp"
            android:layout_height="68dp"
            android:layout_margin="30dp"
            android:background="@drawable/car_background"
            android:padding="16dp"
            android:paddingStart="16dp"
            android:text="4659112"
            android:textAlignment="center"
            android:textSize="26dp"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextView_p"
            app:layout_constraintVertical_bias="0.684"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/TextView_p"
            android:layout_width="359dp"
            android:layout_height="68dp"
            android:background="@drawable/car_background"
            android:padding="16dp"
            android:paddingStart="16dp"
            android:text="4659112"
            android:textAlignment="center"
            android:textSize="26dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="113dp" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="میان دوره "
            android:textColor="@color/white"
            android:textSize="20dp"
            android:layout_margin="20dp"
            app:layout_constraintBottom_toTopOf="@+id/TextView_p"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="پایان دوره"
            android:layout_margin="20dp"

            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toTopOf="@+id/TextView_m"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
